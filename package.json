  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "build": "^0.1.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.20.2",
    "migrate": "^1.6.2",
    "morgan": "^1.9.1",
    "pg": "^7.14.0",
    "postgrator": "^3.10.2",
    "react-dev-utils": "^9.1.0",
    "uglify-js": "^3.6.9",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
